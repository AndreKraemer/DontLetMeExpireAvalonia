<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:converters="clr-namespace:DontLetMeExpireAvalonia.Converters">
	
  <Design.PreviewWith>
	  <StackPanel>
		  <StackPanel Spacing="17" Orientation="Horizontal">
			  <PathIcon Data="{StaticResource Menu}"
						Width="14" />
			  <TextBlock Text="Menu" />
		  </StackPanel>
		  <StackPanel Spacing="17" Orientation="Horizontal">
			  <PathIcon Data="{StaticResource Home}"
						Width="14" />
			  <TextBlock Text="Home" />
		  </StackPanel>
		  
	  </StackPanel>
  </Design.PreviewWith>

	<Style>
		<Style.Resources>
			<StreamGeometry x:Key="Menu">M120-240v-80h720v80H120Zm0-200v-80h720v80H120Zm0-200v-80h720v80H120Z</StreamGeometry>
			<StreamGeometry x:Key="Home">M240-200h120v-240h240v240h120v-360L480-740 240-560v360Zm-80 80v-480l320-240 320 240v480H520v-240h-80v240H160Zm320-350Z</StreamGeometry>
		</Style.Resources>
	</Style>
</Styles>
